generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Stats {
  id           String @id @default(uuid())
  events       Json
  gysSessionId String
}

model User {
  id       String @id @default(uuid())
  username String
  email    String
  password String
  key      String @unique @default(cuid())
  Tag      Tag[]
}

model Tag {
  id       String @id @default(uuid())
  author   User   @relation(fields: [authorId], references: [id])
  authorId String
}
